<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-4 posts-new-container">
        <h1 class="text-center text-white">新規投稿</h1>
        <h3></h3>
        <%= form_for @map do |f| %>
        <%= render 'layouts/error_messages', model: f.object %>
          <div class="form-group">
            <%= f.label :"住所" %>
            <%= f.text_field:address, class: "form-control" %>
          </div>
          <div class="form-group">
            <%= f.submit "投稿", class: "btn btn-block btn-primary" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
