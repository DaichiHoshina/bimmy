<div class="wrapper">
  <div class="users-container">
    <div class="row">
      <% @users.each do |user| %>
        <div class="col-md-offset-3 col-md-6 users-list-container">
          <div class="users-list-card">
            <p><%= link_to "#{user.name}",user_path(user.id) %></p>
            <div class="wuser-image">
              <%= image_tag user.image.to_s %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
